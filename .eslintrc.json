{
    "extends": "airbnb-base",
    "rules": {
        "no-console": 0,
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    },
    "plugins": [
        "mocha",
        "node"
    ],
    "env":{
        "es6": true,
        "mocha": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    }
}